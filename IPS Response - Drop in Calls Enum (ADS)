      <chart>
        <title>by Routername</title>
        <search>
          <query>index="policyguru_data" resourceId="enum*"
| timechart span=1d count by resourceId</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
